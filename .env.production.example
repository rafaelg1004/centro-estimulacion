# Archivo .env.production para el frontend React
# Coloca este archivo en centro-estimulacion/.env.production

# URL de la API en producción
REACT_APP_API_URL=http://tu-ec2-host.amazonaws.com:5000

# URL base para las imágenes
REACT_APP_IMAGES_URL=http://tu-ec2-host.amazonaws.com:5000

# Modo de producción
NODE_ENV=production

# Configuración para build
GENERATE_SOURCEMAP=false

# Configuración de proxy (opcional)
REACT_APP_PROXY_URL=http://tu-ec2-host.amazonaws.com:5000
